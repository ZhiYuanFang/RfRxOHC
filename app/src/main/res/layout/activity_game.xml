<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:binding="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="user1"
            type="xyz.ttyz.tourfrxohc.models.UserModel" />

        <variable
            name="user2"
            type="xyz.ttyz.tourfrxohc.models.UserModel" />

        <variable
            name="user3"
            type="xyz.ttyz.tourfrxohc.models.UserModel" />

        <variable
            name="user4"
            type="xyz.ttyz.tourfrxohc.models.UserModel" />

        <variable
            name="user5"
            type="xyz.ttyz.tourfrxohc.models.UserModel" />

        <variable
            name="user6"
            type="xyz.ttyz.tourfrxohc.models.UserModel" />

        <variable
            name="user7"
            type="xyz.ttyz.tourfrxohc.models.UserModel" />

        <variable
            name="user8"
            type="xyz.ttyz.tourfrxohc.models.UserModel" />

        <variable
            name="user9"
            type="xyz.ttyz.tourfrxohc.models.UserModel" />

    </data>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="5">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="2"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                tools:ignore="NestedWeights">
                <include layout="@layout/layout_game_left"
                    binding:user="@{user1}"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <include layout="@layout/layout_game_right"
                    binding:user="@{user6}"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="2"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                tools:ignore="NestedWeights">
                <include layout="@layout/layout_game_left"
                    binding:user="@{user2}"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <include layout="@layout/layout_game_right"
                    binding:user="@{user7}"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="2"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                tools:ignore="NestedWeights">
                <include layout="@layout/layout_game_left"
                    binding:user="@{user3}"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <include layout="@layout/layout_game_right"
                    binding:user="@{user8}"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="2"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                tools:ignore="NestedWeights">
                <include layout="@layout/layout_game_left"
                    binding:user="@{user4}"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <include layout="@layout/layout_game_right"
                    binding:user="@{user9}"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:weightSum="2"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                tools:ignore="NestedWeights">
                <include layout="@layout/layout_game_left"
                    binding:user="@{user5}"/>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</layout>